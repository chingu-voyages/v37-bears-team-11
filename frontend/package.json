        "prettier": "2.5.1",
        "tailwindcss": "^3.0.23"
    },
    "prettier": {
        "printWidth": 110,
        "bracketSpacing": true,
        "trailingComma": "es5",
        "semi": false,
        "jsxSingleQuote": true,
        "singleQuote": true,
        "tabWidth": 4
    },
